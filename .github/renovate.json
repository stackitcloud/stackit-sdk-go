{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "ignorePresets": [
        ":ignoreModulesAndTests"
    ],
    "prHourlyLimit": 0,
    "repositories": [
        "stackitcloud/stackit-sdk-go"
    ],
    "enabledManagers": [
        "gomod",
        "github-actions"
    ],
    "postUpdateOptions": [
        "gomodTidy"
    ]
}