{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        ":dependencyDashboard"
    ],
    "repositories": [
        "stackitcloud/stackit-sdk-go"
    ],
    "recreateWhen": "always",
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "baseBranches": [
        "hs/renovate-v3"
    ],
    "enabledManagers": [
        "gomod",
        "github-actions"
    ],
    "postUpdateOptions": [
        "gomodTidy"
    ]
}