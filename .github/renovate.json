{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        ":dependencyDashboard"
    ],
    "repositories": [
        "stackitcloud/stackit-sdk-go"
    ],
    "enabledManagers": [
        "gomod",
        "github-actions"
    ],
    "baseBranches": [
        "hs/renovate-v2"
    ],
    "gomod": {
        "packageRules": [
            {
                "matchPackagePatterns": [
                    ".*"
                ],
                "groupName": "go-package"
            }
        ]
    },
    "postUpdateOptions": [
        "gomodTidy"
    ]
}